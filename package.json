{
  "name": "api.bandmatch.io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "DEBUG=api.bandmatch.io:* node ./bin/www",
    "start:production": "NODE_ENV=production node bin/www",
    "init": "sh initialise.sh",
    "lint": "npx standard --fix",
    "docs": "node node_modules/@bandmatch/docbuddyjs",
    "git:commit": "sh scripts/GitCommit.sh",
    "git:push": "sh scripts/GitPush.sh",
    "ver:bump:patch": "standard-version",
    "ver:bump:minor": "standard-version --release-as minor",
    "ver:bump:major": "standard-version --release-as major",
    "db:migrate:dev": "NODE_ENV=development migrate-mongo up",
    "db:rollback:dev": "NODE_ENV=development migrate-mongo down",
    "db:migrate:production": "NODE_ENV=production migrate-mongo up",
    "db:rollback:production": "NODE_ENV=production migrate-mongo down",
    "db:preupdate": "migrate-mongo create"
  },
  "dependencies": {
    "@airbrake/node": "^1.4.1",
    "@bandmatch/docbuddyjs": "github:TCroasdale/docBuddyJS#master",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "config": "^3.3.2",
    "connect-mongodb-session": "^2.4.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "debug": "^2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.10.7",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.11",
    "nodemailer-html-to-text": "^3.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  }
}
